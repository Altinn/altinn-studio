    "axios": "1.3.4",
    "classnames": "2.3.2",
    "history": "5.3.0",
    "i18next": "22.4.11",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.9.0",
