{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Maskinporten": {
        "MetadataAddresses": [
            "https://test.maskinporten.no/.well-known/oauth-authorization-server",
            "https://maskinporten.no/.well-known/oauth-authorization-server"
        ],
        "RequiredScope": "altinn:studio/gateway",
