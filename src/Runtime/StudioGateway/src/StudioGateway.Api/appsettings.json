{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Maskinporten": {
        "MetadataAddresses": [
            "https://test.maskinporten.no/.well-known/oauth-authorization-server",
            "https://maskinporten.no/.well-known/oauth-authorization-server"
        ],
        "RequiredScope": "altinn:studio/gateway",
        "ClientMetadataAddress": "https://test.maskinporten.no/.well-known/oauth-authorization-server"
    },
    "AlertsClientSettings": {
        "Provider": "Grafana"
    },
    "MetricsClientSettings": {
        "Provider": "AzureMonitor"
    },
    "StudioEnvironments": {
        "staging": {
            "Url": "https://staging.altinn.studio/"
        },
        "dev": {
            "Url": "https://dev.altinn.studio/"
        },
        "prod": {
            "Url": "https://altinn.studio/"
        }
    }
}
