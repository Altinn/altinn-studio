    },
    "AlertsClientSettings": {
        "Provider": "grafana"
    },
    "MetricsClientSettings": {
        "Provider": "azuremonitor"
    },
    "StudioClientSettings": {
        "BaseUrl": "https://altinn.studio/designer/api"
    }
}
