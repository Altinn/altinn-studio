  <ItemGroup>
    <ProjectReference Include="../WorkflowEngine.Models/WorkflowEngine.Models.csproj" />
  </ItemGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="$(AssemblyName).Tests" />
  </ItemGroup>

</Project>
