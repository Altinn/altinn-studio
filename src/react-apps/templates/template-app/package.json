{
  "name": "template-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}",
    "clean": "rimraf dist compiled",
    "format": "prettier-tslint fix src/**/*.{ts,tsx}",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress",
    "lintfix": "tslint -c tslint.json 'src/**/*.{ts,tsx} --fix"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "resolutions": {
    "@types/react": "16.9.2"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-grid-layout": "0.16.6",
    "react-hot-loader": "4.3.1",
    "react-modal": "3.4.5",
    "react-redux": "7.1.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "webpack": "4.12.0",
    "webpack-dev-server": "3.1.4"
  },
  "devDependencies": {
    "@types/enzyme": "3.9.4",
    "@types/jest": "23.3.14",
    "@types/react": "16.9.2",
    "@types/react-modal": "3.8.1",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.1",
    "@types/react-test-renderer": "16.8.1",
    "@types/redux-mock-store": "^1.0.0",
    "altinn-designsystem": "2.0.19",
    "awesome-typescript-loader": "5.0.0",
    "babel-jest": "^23.6.0",
    "cross-env": "5.1.6",
    "css-loader": "2.1.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-environment-enzyme": "7.0.2",
    "jest-enzyme": "7.0.2",
    "mini-css-extract-plugin": "0.4.0",
    "prettier": "1.16.4",
    "prettier-tslint": "0.4.2",
    "react-test-renderer": "^16.4.0",
    "redux-mock-store": "^1.5.3",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "23.10.5",
    "ts-loader": "4.4.1",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack-cli": "3.0.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testRegex": ".*.(__tests__).*.(.spec|test.).(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
