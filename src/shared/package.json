{
  "name": "shared",
  "version": "0.1.2",
  "private": true,
  "author": "Altinn",
  "license": "3-Clause BSD",
  "scripts": {
    "test": "jest",
    "lint": "tslint -c ./../tslint.json src/**/*.{ts,tsx} __tests__/**/*.{ts,tsx}",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "resolutions": {
    "@types/react": "16.9.32"
  },
  "dependencies": {
    "@babel/polyfill": "~7.8.7",
    "@material-ui/core": "~4.9.9",
    "@material-ui/icons": "~4.9.1",
    "axios": "~0.19.2",
    "classnames": "~2.2.6",
    "core-js": "~3.6.4",
    "diff": "~4.0.2",
    "dompurify": "~2.0.8",
    "marked": "~0.7.0",
    "moment": "~2.24.0",
    "react-content-loader": "~5.0.3",
    "react-html-parser": "~2.0.2",
    "react-router-dom": "~5.1.2",
    "react-truncate-markup": "~4.1.1",
    "typescript": "~3.8.3"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/preset-env": "~7.9.0",
    "@babel/preset-react": "~7.9.4",
    "@types/classnames": "~2.2.10",
    "@types/diff": "~4.0.2",
    "@types/dompurify": "~2.0.1",
    "@types/enzyme": "~3.10.5",
    "@types/jest": "~25.2.1",
    "@types/jsdom": "~16.2.0",
    "@types/marked": "~0.7.3",
    "@types/react": "~16.9.32",
    "@types/react-dom": "~16.9.6",
    "@types/react-html-parser": "~2.0.1",
    "@types/react-router-dom": "~5.1.3",
    "@types/react-test-renderer": "~16.9.2",
    "altinn-designsystem": "~2.7.4",
    "babel-jest": "~25.2.6",
    "babel-loader": "~8.1.0",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.2",
    "jest": "~25.2.7",
    "jest-environment-enzyme": "~7.1.2",
    "jest-enzyme": "~7.1.2",
    "jest-junit": "~10.0.0",
    "jsdom": "~16.2.2",
    "react": "~16.13.1",
    "react-docgen-typescript": "~1.16.3",
    "react-dom": "~16.13.1",
    "react-styleguidist": "~11.0.5",
    "react-test-renderer": "~16.13.1",
    "ts-jest": "~25.3.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ]
  }
}
