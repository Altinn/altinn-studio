{
  "name": "altinn-app-frontend",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchall": "jest --watchAll",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled",
    "webpack-watch": "cross-env NODE_ENV=development webpack --mode=development --config webpack.config.development.js --watch --progress",
    "styleguidist:run": "yarn dlx styleguidist server",
    "styleguidist:build": "yarn dlx styleguidist build"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@altinn/altinn-design-system": "^0.2.0",
    "@babel/polyfill": "^7.12.1",
    "@date-io/moment": "1.3.13",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "4.11.5",
    "@reduxjs/toolkit": "^1.8.3",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.27.2",
    "dot-object": "^2.1.4",
    "jsonpointer": "5.0.1",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-content-loader": "^6.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.7.1",
    "react-number-format": "^4.9.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.3",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.6",
    "typescript": "^4.7.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.3",
    "@types/dot-object": "^2.1.2",
    "@types/jest": "^28.1.5",
    "@types/react": "17.0.46",
    "@types/react-dom": "^17.0.17",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.3",
    "@types/uuid": "8.3.4",
    "altinn-designsystem": "^3.1.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.19.0",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.12",
    "html-react-parser": "^1.4.14",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^13.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "msw": "^0.44.1",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "react-styleguidist": "^11.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "4.0.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.6",
    "ts-loader": "^9.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\./applicationMetadataMock.json": "<rootDir>/__mocks__/applicationMetadataMock.json",
      "^altinn-shared/(.*)$": "<rootDir>/../shared/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1",
      "^testUtils$": "<rootDir>/testUtils.tsx",
      "^uuid$": "<rootDir>/../node_modules/uuid/dist/index.js"
    },
    "testRegex": "(/__tests__/.*|.*.(test|spec)).(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "!__tests__/**/*",
      "src/**/*.{ts,tsx}"
    ],
    "testEnvironmentOptions": {
      "url": "https://altinn3local.no/ttd/test"
    },
    "testEnvironment": "jsdom"
  }
}
