								},
							"referanse" : {
									"type" : "string"
								},
							"postadresse" : {
									"$ref" : "#/definitions/Adresse"
								},
							"ordrelinje" : {
									"type" : "array",
									"minItems" : 1,
									"items" : {
											"$ref" : "#/definitions/Ordrelinje"
										}
								}
						},
					"required" : [
							"orderid",
							"dato",
							"referanse",
							"postadresse",
							"ordrelinje"
						]
				},
