								},
							"adresse" : {
									"type" : "string"
								},
							"postNummber" : {
									"type" : "integer"
								},
							"postSted" : {
									"type" : "string"
								}
						},
					"required" : [
							"navn",
							"adresse",
							"postNummber",
							"postSted"
						]
				}
		}
}
