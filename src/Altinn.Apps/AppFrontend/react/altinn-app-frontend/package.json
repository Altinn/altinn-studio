{
  "name": "altinn-app-frontend",
  "version": "3.33.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchall": "jest --watchAll",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled",
    "webpack-watch": "cross-env NODE_ENV=development webpack --mode=development --config webpack.config.development.js --watch --progress",
    "styleguidist:run": "yarn dlx styleguidist server",
    "styleguidist:build": "yarn dlx styleguidist build"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@date-io/moment": "1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "4.11.4",
    "@reduxjs/toolkit": "^1.8.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.26.1",
    "dot-object": "^2.1.4",
    "immutability-helper": "2.7.0",
    "jsonpointer": "5.0.0",
    "moment": "^2.29.2",
    "react": "^17.0.2",
    "react-content-loader": "^6.2.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.7.1",
    "react-number-format": "^4.9.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "typescript": "^4.6.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/dot-object": "^2.1.2",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.1",
    "@types/react": "17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.3",
    "@types/uuid": "8.3.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "altinn-designsystem": "^3.1.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.12.0",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "msw": "^0.39.2",
    "react-styleguidist": "^11.2.0",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "4.0.4",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\./applicationMetadataMock.json": "<rootDir>/__mocks__/applicationMetadataMock.json",
      "^altinn-shared/(.*)$": "<rootDir>/../shared/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "(/__tests__/.*|.*.(test|spec)).(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "!__tests__/**/*",
      "src/**/*.{ts,tsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "jsdom",
    "testURL": "https://altinn3local.no/ttd/test"
  }
}
