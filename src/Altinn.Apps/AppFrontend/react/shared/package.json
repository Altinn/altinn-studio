    "core-js": "^3.6.4",
    "diff": "^4.0.2",
    "dompurify": "^2.0.8",
    "marked": "^0.8.2",
    "moment": "^2.24.0",
    "react-content-loader": "^4.3.4",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "^4.3.1",
    "react-truncate-markup": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/classnames": "^2.2.10",
    "@types/diff": "^3.5.3",
    "@types/dompurify": "^0.0.33",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^23.3.14",
    "@types/jsdom": "^12.2.4",
