    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^6.4.0",
    "jsdom": "^13.2.0",
    "react": "^16.13.1",
    "react-docgen-typescript": "^1.16.3",
