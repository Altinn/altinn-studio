{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schema.json",
  "type": "object",
  "@xsdNamespaces": {
    "xsd": "http://www.w3.org/2001/XMLSchema",
    "xsi": "http://www.w3.org/2001/XMLSchema-instance"
  },
  "@xsdSchemaAttributes": {
    "AttributeFormDefault": "Unqualified",
    "ElementFormDefault": "Qualified",
    "BlockDefault": "None",
    "FinalDefault": "None"
  },
  "properties": {
    "Root": {
      "allOf": [
        {
          "allOf": [
            {
              "properties": {
                "t1": {
                  "$ref": "#/definitions/stringMinMaxLengthRestrictions"
                },
                "t2": {
                  "$ref": "#/definitions/stringLengthRestrictions"
                },
                "t3": {
                  "$ref": "#/definitions/stringEnumRestrictions"
                },
                "t4": {
                  "$ref": "#/definitions/stringPatternRestrictions"
                },
                "n1": {
                  "$ref": "#/definitions/numberRestrictions"
                },
                "n2": {
                  "$ref": "#/definitions/numberRestrictions2"
                },
                "f1": {
                  "$ref": "#/definitions/numberRestrictionsFractional0"
                },
                "f2": {
                  "$ref": "#/definitions/numberRestrictionsFractional1"
                },
                "f3": {
                  "$ref": "#/definitions/numberRestrictionsFractional2"
                },
                "f4": {
                  "$ref": "#/definitions/numberRestrictionsFractional3"
                },
                "f5": {
                  "$ref": "#/definitions/numberRestrictionsFractional4"
                },
                "f6": {
                  "$ref": "#/definitions/numberRestrictionsFractional5"
                }
              },
              "required": [
                "t1",
                "t2",
                "t3",
                "t4",
                "n1",
                "n2",
                "f1",
                "f2",
                "f3",
                "f4",
                "f5",
                "f6"
              ]
            }
          ],
          "@xsdStructure": "sequence"
        }
      ]
    }
  },
  "definitions": {
    "stringMinMaxLengthRestrictions": {
      "allOf": [
        {
          "type": "string",
          "@xsdType": "string"
        },
        {
          "maxLength": 20,
          "minLength": 5
        }
      ]
    },
    "stringLengthRestrictions": {
      "allOf": [
        {
          "type": "string",
          "@xsdType": "string"
        },
        {
          "maxLength": 10,
          "minLength": 10
        }
      ]
    },
    "stringEnumRestrictions": {
      "allOf": [
        {
          "type": "string",
          "@xsdType": "string"
        },
        {
          "enum": [
            "v1",
            "v2"
          ]
        }
      ]
    },
    "stringPatternRestrictions": {
      "allOf": [
        {
          "type": "string",
          "@xsdType": "string"
        },
        {
          "pattern": "^\\d\\.\\d\\.\\d$"
        }
      ]
    },
    "numberRestrictions": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "maxLength": 10,
          "maximum": 100,
          "minimum": -100
        }
      ]
    },
    "numberRestrictions2": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "maxLength": 10,
          "exclusiveMaximum": 100,
          "exclusiveMinimum": -100
        }
      ]
    },
    "numberRestrictionsFractional0": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "multipleOf": 1
        }
      ]
    },
    "numberRestrictionsFractional1": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "multipleOf": 0.1
        }
      ]
    },
    "numberRestrictionsFractional2": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "multipleOf": 0.01
        }
      ]
    },
    "numberRestrictionsFractional3": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "multipleOf": 0.001
        }
      ]
    },
    "numberRestrictionsFractional4": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "multipleOf": 0.0001
        }
      ]
    },
    "numberRestrictionsFractional5": {
      "allOf": [
        {
          "type": "number",
          "@xsdType": "decimal"
        },
        {
          "multipleOf": 0.00001
        }
      ]
    }
  }
}