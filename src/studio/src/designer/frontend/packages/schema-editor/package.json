{
  "name": "@altinn/schema-editor",
  "version": "0.1.0",
  "private": true,
  "description": "JSON schema editor",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.7.2",
    "axios": "^0.26.0",
    "jsonpointer": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^12.8.3",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.22",
    "@types/redux-mock-store": "^1.0.3",
    "axios": "^0.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "enzyme": "^3.11.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "jsonpointer": "^4.1.0",
    "mini-css-extract-plugin": "^1.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "react-styleguidist": "^11.2.0",
    "redux": "^4.1.2",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --progress --watch",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../../shared/$1"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
