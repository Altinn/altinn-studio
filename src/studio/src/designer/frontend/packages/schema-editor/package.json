{
  "name": "@altinn/schema-editor",
  "version": "0.1.0",
  "private": true,
  "description": "JSON schema editor",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@mui/lab": "5.0.0-alpha.104"
  },
  "peerDependencies": {
    "@altinn/altinn-design-system": "^0.11.1 || ^0.12.0",
    "@altinn/schema-model": "*",
    "@mui/icons-material": "5.10.9",
    "@mui/material": "5.10.10",
    "@reduxjs/toolkit": "^1.8.3",
    "react": "^18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "redux-saga": "1.2.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "1.8.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.1.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "7.1.24",
    "@types/redux-mock-store": "1.0.3",
    "ajv": "8.11.0",
    "axios": "0.27.2",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.25.0",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "jest-junit": "14.0.1",
    "jsonpointer": "5.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.4",
    "react-scripts": "5.0.1",
    "react-styleguidist": "11.2.0",
    "redux": "4.2.0",
    "redux-mock-store": "1.5.4",
    "redux-saga-test-plan": "4.0.6",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --progress --watch",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "node_modules(\\\\|/)(@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)(\\\\|/).+\\.(j|t)sx?$": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../../shared/$1",
      "^@altinn/schema-model/(.*)": "<rootDir>/../schema-model/$1"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules(\\\\|/)(?!@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
