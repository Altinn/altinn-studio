{
  "name": "app-development",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress --mode development"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@reduxjs/toolkit": "1.8.5",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "core-js": "3.25.1",
    "history": "5.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-modal": "3.15.1",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-select": "5.4.0",
    "react-truncate-markup": "5.1.2",
    "react-use": "17.4.0",
    "redux": "4.2.0",
    "redux-saga": "1.2.1",
    "reselect": "4.1.6",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.19.0",
    "@babel/preset-react": "7.18.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/classnames": "2.3.1",
    "@types/jest": "29.0.1",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "3.13.1",
    "@types/react-redux": "7.1.24",
    "@types/redux-mock-store": "1.0.3",
    "altinn-designsystem": "2.7.12",
    "babel-jest": "29.0.3",
    "body-parser": "1.20.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.23.0",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "jest-junit": "14.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "monaco-editor-webpack-plugin": "4.2.0",
    "prettier": "2.7.1",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "source-map-loader": "4.0.0",
    "style-loader": "3.3.1",
    "svg-inline-loader": "0.8.2",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "29.0.0",
    "ts-loader": "9.3.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.0"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)": "ts-jest",
      "node_modules(\\\\|/)(react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)(\\\\|/).+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules(\\\\|/)(?!react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "monaco-editor": "<rootDir>/test/__mocks__/fileMock.js",
      "^utils/(.*)": "<rootDir>/utils/$1",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^ux-editor/(.*)": "<rootDir>/../ux-editor/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1",
      "^common/(.*)": "<rootDir>/common/$1",
      "^test/(.*)": "<rootDir>/test/$1",
      "^store/(.*)": "<rootDir>/store/$1",
      "^uuid$": "<rootDir>/../node_modules/uuid/dist/index.js"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
