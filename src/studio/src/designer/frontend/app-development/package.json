{
  "name": "app-development",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "lint": "tslint -c ./../tslint.json src/**/*.{ts,tsx} __tests__/**/*.{ts,tsx}",
    "clean": "rimraf dist compiled",
    "format": "prettier-tslint fix src/**/*.{ts,tsx}",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress --mode development",
    "lintfix": "tslint -c tslint.json 'src/**/*.{ts,tsx} --fix"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "~7.12.1",
    "@material-ui/core": "~4.12.2",
    "@reduxjs/toolkit": "~1.6.1",
    "@types/domhandler": "^2.4.2",
    "axios": "~0.21.1",
    "classnames": "~2.3.1",
    "core-js": "~3.15.2",
    "moment": "~2.29.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-hot-loader": "~4.13.0",
    "react-modal": "~3.14.3",
    "react-redux": "~7.2.4",
    "react-router-dom": "~5.2.0",
    "react-select": "~4.3.1",
    "react-truncate-markup": "~5.1.0",
    "redux": "~4.1.0",
    "redux-devtools-extension": "~2.13.9",
    "redux-logger": "~3.0.6",
    "redux-saga": "~1.1.3",
    "reselect": "~4.0.0",
    "typescript": "~4.3.5"
  },
  "devDependencies": {
    "@babel/core": "~7.14.8",
    "@babel/preset-env": "~7.14.8",
    "@babel/preset-react": "~7.14.5",
    "@testing-library/jest-dom": "~5.14.1",
    "@testing-library/react": "~12.0.0",
    "@types/classnames": "~2.3.1",
    "@types/enzyme": "~3.10.9",
    "@types/jest": "~26.0.24",
    "@types/react": "~17.0.14",
    "@types/react-dom": "~17.0.9",
    "@types/react-modal": "~3.12.1",
    "@types/react-redux": "~7.1.18",
    "@types/react-router-dom": "~5.1.8",
    "@types/react-test-renderer": "~17.0.1",
    "@types/redux-mock-store": "~1.0.3",
    "altinn-designsystem": "2.7.12",
    "babel-jest": "~27.0.6",
    "cross-env": "~7.0.3",
    "css-loader": "~5.2.6",
    "enzyme": "~3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "~0.6.3",
    "eslint": "~7.31.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "fork-ts-checker-notifier-webpack-plugin": "~4.0.0",
    "fork-ts-checker-webpack-plugin": "~6.2.12",
    "html-loader": "~1.3.2",
    "html-webpack-plugin": "~4.5.2",
    "jest": "~27.0.6",
    "jest-environment-enzyme": "~7.1.2",
    "jest-enzyme": "~7.1.2",
    "jest-junit": "~12.2.0",
    "mini-css-extract-plugin": "~1.6.2",
    "monaco-editor-webpack-plugin": "~4.1.1",
    "prettier": "~2.3.2",
    "prettier-tslint": "~0.4.2",
    "react-test-renderer": "~17.0.2",
    "redux-mock-store": "~1.5.4",
    "resolve-url-loader": "~3.1.4",
    "rimraf": "~3.0.2",
    "source-map-loader": "~1.1.3",
    "style-loader": "~2.0.0",
    "svg-inline-loader": "~0.8.2",
    "terser-webpack-plugin": "~4.2.3",
    "ts-jest": "~27.0.4",
    "ts-loader": "~8.3.0",
    "webpack": "~4.44.2",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "~3.11.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^utils/(.*)": "<rootDir>/utils/$1",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^ux-editor/(.*)": "<rootDir>/../ux-editor/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/test/setupTests.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}