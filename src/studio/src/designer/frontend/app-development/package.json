{
  "name": "app-development",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress --mode development"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "core-js": "^3.23.4",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.1",
    "react-select": "^4.3.1",
    "react-truncate-markup": "^5.1.2",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.6",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^28.1.5",
    "@types/react": "17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.2",
    "@types/redux-mock-store": "^1.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "altinn-designsystem": "2.7.12",
    "babel-jest": "^28.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.16.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.12",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^13.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "monaco-editor-webpack-plugin": "^4.2.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.6",
    "ts-loader": "^9.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "monaco-editor": "<rootDir>/test/__mocks__/fileMock.js",
      "^utils/(.*)": "<rootDir>/utils/$1",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^ux-editor/(.*)": "<rootDir>/../ux-editor/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1",
      "^common/(.*)": "<rootDir>/common/$1",
      "^test/(.*)": "<rootDir>/test/$1",
      "^store/(.*)": "<rootDir>/store/$1",
      "^uuid$": "<rootDir>/../node_modules/uuid/dist/index.js"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
