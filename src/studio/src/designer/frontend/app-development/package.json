{
  "name": "app-development",
  "version": "0.1.2",
  "author": "Altinn",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "@altinn/altinn-design-system": "0.23.0",
    "@mui/material": "5.10.12",
    "@mui/styles": "5.10.10",
    "@reduxjs/toolkit": "1.9.0",
    "axios": "0.27.2",
    "classnames": "2.3.2",
    "history": "5.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.3",
    "react-select": "5.6.0",
    "redux": "4.2.0",
    "redux-saga": "1.2.1",
    "reselect": "4.1.7"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.2",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/react-redux": "7.1.24",
    "@types/redux-mock-store": "1.0.3",
    "cross-env": "7.0.3",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "jest": "29.2.2",
    "jest-environment-jsdom": "29.2.2",
    "jest-junit": "14.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "monaco-editor": "0.34.1",
    "monaco-editor-webpack-plugin": "7.0.1",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "terser-webpack-plugin": "5.3.6",
    "webpack": "5.74.0",
    "webpack-dev-server": "4.11.1"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)": "@swc/jest",
      "node_modules(\\\\|/)(react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend|@?react-leaflet)(\\\\|/).+\\.(j|t)sx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "node_modules(\\\\|/)(?!react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend|@?react-leaflet)"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "monaco-editor": "<rootDir>/test/__mocks__/fileMock.js",
      "^utils/(.*)": "<rootDir>/utils/$1",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^ux-editor/(.*)": "<rootDir>/../ux-editor/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1",
      "^common/(.*)": "<rootDir>/common/$1",
      "^test/(.*)": "<rootDir>/test/$1",
      "^store/(.*)": "<rootDir>/store/$1",
      "^uuid$": "<rootDir>/../node_modules/uuid/dist/index.js"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "license": "3-Clause BSD",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "clean": "rimraf dist compiled",
    "compile-ts": "tsc",
    "depcheck": "echo Checking $npm_package_name && npx depcheck && echo ",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress --mode development"
  }
}
