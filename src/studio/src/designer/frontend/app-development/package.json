{
  "name": "app-development",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "lint": "tslint -c ./../tslint.json src/**/*.{ts,tsx} __tests__/**/*.{ts,tsx}",
    "clean": "rimraf dist compiled",
    "format": "prettier-tslint fix src/**/*.{ts,tsx}",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress --mode development",
    "lintfix": "tslint -c tslint.json 'src/**/*.{ts,tsx} --fix"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "resolutions": {
    "@types/react": "16.9.43"
  },
  "dependencies": {
    "@babel/polyfill": "~7.10.4",
    "@material-ui/core": "~4.11.0",
    "axios": "~0.19.2",
    "classnames": "~2.2.6",
    "core-js": "~3.6.5",
    "immutability-helper": "2.7.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-grid-layout": "~0.18.3",
    "react-hot-loader": "~4.12.21",
    "react-modal": "~3.11.2",
    "react-redux": "~7.2.0",
    "react-router-dom": "~5.2.0",
    "react-select": "~3.1.0",
    "react-truncate-markup": "~5.0.0",
    "redux": "~4.0.5",
    "redux-devtools-extension": "~2.13.8",
    "redux-logger": "~3.0.6",
    "redux-saga": "~1.1.3",
    "reselect": "~4.0.0",
    "typescript": "~3.9.6"
  },
  "devDependencies": {
    "@babel/core": "~7.10.5",
    "@babel/preset-env": "~7.10.4",
    "@babel/preset-react": "~7.10.4",
    "@testing-library/react": "10.4.7",
    "@testing-library/jest-dom": "5.11.1",
    "@types/classnames": "~2.2.10",
    "@types/enzyme": "~3.10.5",
    "@types/jest": "~26.0.4",
    "@types/react": "~16.9.43",
    "@types/react-dom": "~16.9.8",
    "@types/react-modal": "~3.10.6",
    "@types/react-redux": "~7.1.9",
    "@types/react-router-dom": "~5.1.5",
    "@types/react-test-renderer": "~16.9.2",
    "@types/redux-mock-store": "~1.0.2",
    "altinn-designsystem": "~2.7.5",
    "awesome-typescript-loader": "~5.2.1",
    "babel-jest": "~26.1.0",
    "cross-env": "~7.0.2",
    "css-loader": "~3.6.0",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.2",
    "eslint": "~7.4.0",
    "eslint-config-airbnb-base": "~14.2.0",
    "html-loader": "~1.1.0",
    "html-webpack-plugin": "~4.3.0",
    "jest": "~26.1.0",
    "jest-environment-enzyme": "~7.1.2",
    "jest-enzyme": "~7.1.2",
    "jest-junit": "~11.0.1",
    "mini-css-extract-plugin": "~0.9.0",
    "monaco-editor-webpack-plugin": "~1.9.0",
    "prettier": "~2.0.5",
    "prettier-tslint": "~0.4.2",
    "react-test-renderer": "~16.13.1",
    "redux-mock-store": "~1.5.4",
    "resolve-url-loader": "~3.1.1",
    "rimraf": "~3.0.2",
    "source-map-loader": "~1.0.1",
    "style-loader": "~1.2.1",
    "svg-inline-loader": "~0.8.2",
    "ts-jest": "~26.1.2",
    "ts-loader": "~8.0.1",
    "terser-webpack-plugin": "~3.0.6",
    "webpack": "~4.43.0",
    "webpack-dev-server": "~3.11.0",
    "webpack-cli": "~3.3.12"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^ux-editor/(.*)": "<rootDir>/../ux-editor/$1"
    },
    "testRegex": "(test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
