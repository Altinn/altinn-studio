{
  "name": "app-shared",
  "version": "0.1.0",
  "author": "Altinn",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@altinn/altinn-design-system": "0.17.0",
    "@altinn/schema-editor": "0.1.0",
    "@mui/icons-material": "5.10.9",
    "@mui/material": "5.10.10",
    "@mui/styles": "5.10.10",
    "@reduxjs/toolkit": "1.8.6",
    "axios": "0.27.2",
    "classnames": "2.3.2",
    "diff": "5.1.0",
    "dompurify": "2.4.0",
    "marked": "4.1.1",
    "moment": "2.29.4",
    "monaco-editor": "0.34.1",
    "react": "18.2.0",
    "react-content-loader": "6.2.0",
    "react-dom": "18.2.0",
    "react-html-parser": "2.0.2",
    "react-monaco-editor": "0.50.1",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.2",
    "react-select": "5.5.4",
    "redux-saga": "1.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/diff": "5.0.2",
    "@types/dompurify": "2.3.4",
    "@types/jest": "29.2.0",
    "@types/marked": "4.0.7",
    "@types/react": "18.0.21",
    "@types/react-html-parser": "2.0.2",
    "@types/redux-mock-store": "1.0.3",
    "jest": "29.2.1",
    "jest-junit": "14.0.1",
    "msw": "0.47.4",
    "redux-mock-store": "1.5.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "@swc/jest",
      "node_modules(\\\\|/)(@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)(\\\\|/).+\\.(j|t)sx?$": "@swc/jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1",
      "^app-shared/(.*)": "<rootDir>/$1"
    },
    "testRegex": "(/test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules(\\\\|/)(?!@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)"
    ]
  },
  "license": "3-Clause BSD",
  "peerDependencies": {
    "redux": "4.2.0"
  },
  "private": true,
  "scripts": {
    "depcheck": "echo Checking $npm_package_name && npx depcheck && echo ",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand"
  }
}
