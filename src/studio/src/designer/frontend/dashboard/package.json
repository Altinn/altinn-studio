{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "lint": "tslint -c ./../tslint.json src/**/*.{ts,tsx} __tests__/**/*.{ts,tsx}",
    "clean": "rimraf dist compiled",
    "format": "prettier-tslint fix src/**/*.{ts,tsx}",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress",
    "lintfix": "tslint -c tslint.json 'src/**/*.{ts,tsx} --fix"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "resolutions": {
    "@types/react": "16.9.43"
  },
  "dependencies": {
    "@material-ui/core": "~4.11.3",
    "@material-ui/icons": "~4.11.2",
    "@reduxjs/toolkit": "~1.5.1",
    "core-js": "~3.10.2",
    "moment": "~2.29.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-hot-loader": "~4.13.0",
    "react-html-parser": "~2.0.2",
    "react-modal": "~3.13.1",
    "react-redux": "~7.2.3",
    "react-router-dom": "~5.2.0",
    "react-truncate-markup": "~5.1.0",
    "redux": "~4.0.5",
    "redux-devtools-extension": "~2.13.9",
    "redux-logger": "~3.0.6",
    "redux-saga": "~1.1.3",
    "reselect": "~4.0.0",
    "typescript": "~4.2.4"
  },
  "devDependencies": {
    "@babel/core": "~7.13.16",
    "@babel/polyfill": "~7.12.1",
    "@babel/preset-env": "~7.13.15",
    "@babel/preset-react": "~7.13.13",
    "@types/classnames": "~2.3.1",
    "@types/enzyme": "~3.10.8",
    "@types/jest": "~26.0.22",
    "@types/react": "~16.9.56",
    "@types/react-dom": "~16.9.12",
    "@types/react-modal": "~3.12.0",
    "@types/react-redux": "~7.1.16",
    "@types/react-router-dom": "~5.1.7",
    "@types/react-test-renderer": "~16.9.5",
    "@types/redux-mock-store": "~1.0.2",
    "altinn-designsystem": "2.7.12",
    "babel-jest": "~26.6.3",
    "classnames": "~2.3.1",
    "cross-env": "~7.0.3",
    "css-loader": "~5.2.4",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.6",
    "eslint": "~7.24.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "fork-ts-checker-notifier-webpack-plugin": "~3.0.0",
    "fork-ts-checker-webpack-plugin": "~6.2.4",
    "html-loader": "~1.3.2",
    "html-webpack-plugin": "~4.5.2",
    "jest": "~26.6.3",
    "jest-environment-enzyme": "~7.1.2",
    "jest-enzyme": "~7.1.2",
    "jest-junit": "~12.0.0",
    "mini-css-extract-plugin": "~1.5.0",
    "prettier": "~2.2.1",
    "prettier-tslint": "~0.4.2",
    "react-test-renderer": "~16.13.1",
    "redux-mock-store": "~1.5.4",
    "resolve-url-loader": "~3.1.2",
    "rimraf": "~3.0.2",
    "source-map-loader": "~1.1.3",
    "style-loader": "~2.0.0",
    "svg-inline-loader": "~0.8.2",
    "ts-jest": "~26.5.5",
    "ts-loader": "~8.1.0",
    "terser-webpack-plugin": "~4.2.3",
    "webpack": "~4.44.2",
    "webpack-dev-server": "~3.11.2",
    "webpack-cli": "~3.3.12"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../shared/$1"
    },
    "testRegex": "(/test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
