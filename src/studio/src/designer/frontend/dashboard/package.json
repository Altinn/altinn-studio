{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "test": "jest",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@emotion/styled": "11.10.4",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@mui/icons-material": "5.10.3",
    "@mui/material": "5.10.3",
    "@mui/x-data-grid": "5.17.1",
    "@reduxjs/toolkit": "1.8.5",
    "core-js": "3.25.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-html-parser": "2.0.2",
    "react-modal": "3.15.1",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-truncate-markup": "5.1.2",
    "react-use": "17.4.0",
    "redux": "4.2.0",
    "redux-saga": "1.2.1",
    "reselect": "4.1.6",
    "typescript": "4.8.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/classnames": "2.3.1",
    "@types/jest": "29.0.0",
    "@types/lodash-es": "4.17.6",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "3.13.1",
    "@types/redux-mock-store": "1.0.3",
    "altinn-designsystem": "2.7.12",
    "babel-jest": "29.0.2",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.22.0",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "jest-junit": "14.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "msw": "0.47.0",
    "prettier": "2.7.1",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "source-map-loader": "4.0.0",
    "style-loader": "3.3.1",
    "svg-inline-loader": "0.8.2",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "29.0.0",
    "ts-loader": "9.3.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.1",
    "whatwg-fetch": "3.6.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^app/(.*)": "<rootDir>/app/$1",
      "^features/(.*)": "<rootDir>/features/$1",
      "^common/(.*)": "<rootDir>/common/$1",
      "^services/(.*)": "<rootDir>/services/$1",
      "^test/(.*)": "<rootDir>/test/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1"
    },
    "testRegex": "(/test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
