{
  "name": "dashboard",
  "version": "0.1.0",
  "author": "Altinn",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/styles": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@reduxjs/toolkit": "1.8.6",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.2",
    "react-use": "17.4.0",
    "redux": "4.2.0",
    "redux-saga": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/classnames": "2.3.1",
    "@types/jest": "29.2.0",
    "@types/lodash-es": "4.17.6",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/redux-mock-store": "1.0.3",
    "babel-jest": "29.2.1",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "jest": "29.2.1",
    "jest-junit": "14.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "msw": "0.47.4",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-dev-server": "4.11.1",
    "whatwg-fetch": "3.6.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "node_modules(\\\\|/)(@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)(\\\\|/).+\\.(j|t)sx?$": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^app/(.*)": "<rootDir>/app/$1",
      "^features/(.*)": "<rootDir>/features/$1",
      "^common/(.*)": "<rootDir>/common/$1",
      "^services/(.*)": "<rootDir>/services/$1",
      "^test/(.*)": "<rootDir>/test/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1"
    },
    "testRegex": "(/test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules(\\\\|/)(?!@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)"
    ]
  },
  "license": "3-Clause BSD",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build-develop": "cross-env NODE_ENV=development webpack --config webpack.config.development --progress --watch",
    "clean": "rimraf dist compiled",
    "compile-ts": "tsc",
    "depcheck": "echo Checking $npm_package_name && npx depcheck && echo ",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "test": "jest",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.development.js --watch --progress"
  }
}
