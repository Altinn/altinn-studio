{
  "name": "dashboard",
  "version": "0.1.0",
  "author": "Altinn",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/styled": "11.10.5",
    "@mui/material": "5.10.12",
    "@mui/styles": "5.10.10",
    "@mui/x-data-grid": "5.17.10",
    "@reduxjs/toolkit": "1.9.0",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.3",
    "react-use": "17.4.0",
    "redux": "4.2.0",
    "redux-saga": "1.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "@swc/jest",
      "node_modules(\\\\|/)(@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)(\\\\|/).+\\.(j|t)sx?$": "@swc/jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^app-shared/(.*)": "<rootDir>/../shared/$1",
      "^@altinn/schema-editor/(.*)": "<rootDir>/../packages/schema-editor/src/$1"
    },
    "testRegex": "(/test/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules(\\\\|/)(?!@?react-leaflet|react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)"
    ]
  },
  "license": "3-Clause BSD",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ../webpack.config.prod.js",
    "clean": "rimraf dist compiled",
    "compile-ts": "tsc",
    "depcheck": "echo Checking $npm_package_name && npx depcheck && echo ",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ../webpack.config.dev.js --mode development",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand"
  }
}
