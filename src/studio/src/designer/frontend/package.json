{
  "name": "altinn-studio-frontend",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@swc/jest": "0.2.23",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "css-loader": "6.7.1",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "jest-environment-jsdom": "29.2.2",
    "prettier": "2.7.1",
    "source-map-loader": "4.0.1",
    "style-loader": "3.3.1",
    "svg-inline-loader": "0.8.2",
    "ts-loader": "9.4.1",
    "webpack-cli": "4.10.0"
  },
  "packageManager": "yarn@3.2.4",
  "private": true,
  "scripts": {
    "build-app-development": "yarn workspace app-development run build",
    "build-dashboard": "yarn workspace dashboard run build",
    "depcheck": "npx depcheck && yarn workspaces foreach run depcheck",
    "lint": "eslint \"./**/*.ts*\"",
    "start-app-development": "yarn workspace app-development start",
    "start-dashboard": "yarn workspace dashboard start",
    "syncpack": "npx syncpack fix-mismatches && npx syncpack format",
    "test": "yarn workspaces foreach --parallel run test ",
    "test:ci": "yarn workspaces foreach --parallel run test:ci"
  },
  "syncpack": {
    "semverRange": ""
  },
  "workspaces": [
    "dashboard",
    "app-development",
    "shared",
    "ux-editor",
    "packages/schema-editor",
    "packages/schema-model"
  ]
}
