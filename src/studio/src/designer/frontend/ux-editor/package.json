    "@types/classnames": "~2.3.1",
    "@types/deepmerge": "~2.2.0",
    "@types/enzyme": "~3.10.8",
    "@types/jest": "~26.0.23",
    "@types/react": "~17.0.11",
    "@types/react-dom": "~17.0.8",
    "@types/react-modal": "~3.12.0",
    "@types/react-redux": "~7.1.16",
    "@types/react-router-dom": "~5.1.7",
