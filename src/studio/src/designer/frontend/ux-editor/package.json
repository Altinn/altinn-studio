    "enzyme": "~3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "~0.6.2",
    "eslint": "~7.29.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "html-loader": "~1.3.2",
    "html-webpack-plugin": "~4.5.2",
    "jest": "~27.0.5",
    "jest-environment-enzyme": "~7.1.2",
