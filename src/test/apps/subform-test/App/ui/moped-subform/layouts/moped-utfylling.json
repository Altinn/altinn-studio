{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": false,
    "layout": [
      {
        "size": "L",
        "id": "moped-header",
        "type": "Header",
        "textResourceBindings": {
          "title": "moped-header.title"
        }
      },
      {
        "id": "moped-blurb",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "moped-blurb.title"
        }
      },
      {
        "dataModelBindings": {
          "simpleBinding": "RegNo"
        },
        "id": "moped-regno",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-regno.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Merke"
        },
        "id": "moped-merke",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-merke.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Modell"
        },
        "id": "moped-modell",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-modell.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "EkstraInfoCheck"
        },
        "id": "moped-extrainfo-check",
        "type": "RadioButtons",
        "textResourceBindings": {
          "title": "Har du ekstra info?"
        },
        // This requires a network request, and must not have any mapping/queryParameters
        // that makes it non-statically fetchable (for regression testing https://github.com/Altinn/app-frontend-react/issues/3019)
        "optionsId": "janei"
      },
      {
        "dataModelBindings": {
          "simpleBinding": "EkstraInfoData"
        },
        "id": "moped-extrainfo-data",
        "type": "Input",
        "textResourceBindings": {
          "title": "Ekstra info"
        },
        "required": true,
        "hidden": [
          "equals",
          [
            "component",
            "moped-extrainfo-check"
          ],
          false
        ]
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Produksjonsaar"
        },
        "id": "moped-produksjonsaar",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-produksjonsaar.title"
        },
        "formatting": {
          "number": {
            "format": "####"
          }
        },
        "required": true
      },
      {
        "id": "moped-ekstrainfo-panel",
        "type": "Panel",
        "textResourceBindings": {
          "title": "Ekstra info?",
          "body": [
            "if",
            [
              "equals",
              [
                "component",
                "moped-extrainfo-check"
              ],
              true
            ],
            "Du har valgt Ã¥ melde inn ekstra informasjon",
            "else",
            "Ingen ekstra informasjon oppgitt"
          ]
        },
        "variant": "info",
        "showIcon": false
      },
      {
        "id": "button-group-moped",
        "type": "ButtonGroup",
        "children": [
          "subform-moped-cancelButton",
          "subform-moped-exitButton"
        ]
      },
      {
        "id": "subform-moped-cancelButton",
        "type": "CustomButton",
        "textResourceBindings": {
          "title": "general.cancel"
        },
        "actions": [
          {
            "type": "ClientAction",
            "id": "closeSubform"
          }
        ],
        "buttonSize": "sm",
        "buttonStyle": "secondary",
        "buttonColor": "second"
      },
      {
        "id": "subform-moped-exitButton",
        "type": "CustomButton",
        "actions": [
          {
            "type": "ClientAction",
            "id": "closeSubform",
            "validation": {
              "page": "all",
              "show": [
                "All"
              ]
            }
          }
        ],
        "buttonSize": "sm"
      }
    ]
  }
}