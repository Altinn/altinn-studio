{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "layout": [
      {
        "id": "TablePage-NavigationBar",
        "type": "NavigationBar"
      },
      {
        "size": "h2",
        "id": "TablePage-Header",
        "type": "Header",
        "textResourceBindings": {
          "title": "Table Examples"
        }
      },
      {
        "id": "Table1",
        "type": "SimpleTable",
        "title": "Schmitle",
        "dataModelBindings": {
          "tableData": "repeatingGroup"
        },
        "enableDelete": true,
        "columns": [
          {
            "header": "table.header.name",
            "accessors": ["name", "points", "date"]
          },
          {
            "header": "table.header.name",
            "accessors": ["name"],
            "component": {
              "type": "link",
              "hrefPath": "name",
              "textPath": "points",
              "openInNewTab": true
            }
          },
          {
            "header": "table.header.points",
            "accessors": ["points"]
          },
          {
            "header": "table.header.date",
            "accessors": ["date"]
          }
        ]
      },
      {
        "size": "h2",
        "id": "TablePage-SummaryHeader",
        "type": "Header",
        "textResourceBindings": {
          "title": "Summary"
        }
      },
      {
        "id": "TablePage-Summary2",
        "type": "Summary2",
        "target": {
          "type": "component",
          "id": "Table1"
        }
      },
      {
        "id": "Table-NavigationButtons",
        "type": "NavigationButtons",
        "showBackButton": true,
        "textResourceBindings": {
          "next": "Neste",
          "back": "Forrige"
        }
      }
    ]
  }
}