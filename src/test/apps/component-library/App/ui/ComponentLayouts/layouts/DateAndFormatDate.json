        },
        "dataModelBindings": {
          "simpleBinding": "Dates.DateTime"
        },
        "timeStamp": true,
        "grid": {
          "md": 4
        }
      },
      {
        "id": "dates-DatePicker-DateOnly",
        "type": "Datepicker",
        "textResourceBindings": {
          "title": "Datovelger u/timestamp",
          "description": "Lagrer mot DateOnly"
        },
        "dataModelBindings": {
          "simpleBinding": "Dates.DateOnly"
        },
        "timeStamp": false,
        "grid": {
          "md": 4
        }
      },
      {
        "id": "DateAndFormatDate-NavigationButtons",
        "type": "NavigationButtons",
        "showBackButton": true,
        "textResourceBindings": {},
        "validateOnNext": {
          "page": "current",
          "show": ["AllExceptRequired"]
        }
      }
    ]
