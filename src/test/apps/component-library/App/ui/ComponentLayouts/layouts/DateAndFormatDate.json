{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": false,
    "layout": [
      {
        "id": "DateAndFormatDate-NavigationBar",
        "type": "NavigationBar"
      },
      {
        "id": "datesDate",
        "type": "Dropdown",
        "textResourceBindings": {
          "title": "Velg dato/tid"
        },
        "dataModelBindings": {
          "simpleBinding": "Dates.SetDate"
        },
        "options": [
          { "label": "Velg en verdi for 책 overstyre...", "value": "-" },
          { "label": "N책", "value": "now" },
          { "label": "Skudd책rsdagen 2020", "value": "2020-02-29 12:00:00" },
          { "label": "Veldig tett p책 2022", "value": "2021-12-31 23:59:59.9999999" },
          { "label": "Midnatt i en annen tidssone", "value": "2020-05-17 00:00:00-08:00" },
          { "label": "Midnatt i UTC", "value": "2020-05-17T00:00:00Z" }
        ]
      },
      {
        "id": "dates-Text-String",
        "type": "Text",
        "textResourceBindings": {
          "title": "Text",
          "description": "Data fra streng"
        },
        "value": ["dataModel", "Dates.String"],
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-Text-DateTime",
        "type": "Text",
        "textResourceBindings": {
          "title": "Text-komponenten",
          "description": "Data fra DateTime"
        },
        "value": ["dataModel", "Dates.DateTime"],
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-Text-DateOnly",
        "type": "Text",
        "textResourceBindings": {
          "title": "Text-komponenten",
          "description": "Data fra DateOnly"
        },
        "value": ["dataModel", "Dates.DateOnly"],
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-Date-String",
        "type": "Date",
        "textResourceBindings": {
          "title": "Date",
          "description": "Data fra streng"
        },
        "value": ["dataModel", "Dates.String"],
        "format": "dd.MM.yyyy HH:mm:ss",
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-Date-DateTime",
        "type": "Date",
        "textResourceBindings": {
          "title": "Date",
          "description": "Data fra DateTime"
        },
        "value": ["dataModel", "Dates.DateTime"],
