        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-Date-DateOnly",
        "type": "Date",
        "textResourceBindings": {
          "title": "Date",
          "description": "Data fra DateOnly"
        },
        "value": ["dataModel", "Dates.DateOnly"],
        "format": "dd.MM.yyyy HH:mm:ss",
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-TextFormat-String",
        "type": "Text",
        "textResourceBindings": {
          "title": "Text m/formatDate",
          "description": "Data fra streng"
        },
        "value": ["formatDate", ["dataModel", "Dates.String"], "dd.MM.yyyy HH:mm:ss"],
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
        "id": "dates-TextFormat-DateTime",
        "type": "Text",
        "textResourceBindings": {
          "title": "Text m/formatDate",
          "description": "Data fra DateTime"
        },
        "value": ["formatDate", ["dataModel", "Dates.DateTime"], "dd.MM.yyyy HH:mm:ss"],
        "direction": "vertical",
        "grid": {
          "xs": 4
        }
      },
      {
