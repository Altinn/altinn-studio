{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "layout": [
      {
        "id": "AccordionGroupPage-NavigationBar",
        "type": "NavigationBar"
      },
      {
        "id": "accordion-group",
        "type": "AccordionGroup",
        "children": [
          "accordion1",
          "accordion2"
        ]
      },
      {
        "id": "accordion1",
        "type": "Accordion",
        "textResourceBindings": {
          "title": "Accordion 1 Title"
        },
        "children": [
          "paragraph1", "AccordionGroupPage-Input1"
        ]
      },
      {
        "id": "accordion2",
        "type": "Accordion",
        "textResourceBindings": {
          "title": "Accordion 2 Title"
        },
        "children": [
          "paragraph2", "AccordionGroupPage-Input2"
        ]
      },
      {
        "id": "paragraph1",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "This is the first paragraph."
        }
      },
      {
        "id": "paragraph2",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "This is the second paragraph."
        }
      },

      {
        "id": "AccordionGroupPage-Input1",
        "type": "Input",
        "textResourceBindings": {
          "title": "Accordion Test 1"
        },
        "dataModelBindings": {
          "simpleBinding": "shortAnswerInput"
        }
      },

      {
        "id": "AccordionGroupPage-Input2",
        "type": "Input",
        "textResourceBindings": {
          "title": "Accordion Test 2"
        },
        "dataModelBindings": {
          "simpleBinding": "shortAnswerInput"
        }
      },
      
      {
        "id": "AccordionGroupPage-Summary2",
        "type": "Summary2",
        "target": {
          "type": "component",
          "id": "accordion-group"
        }
      },
      {
        "id": "AccordionGroupPage-NavigationButtons",
        "type": "NavigationButtons",
        "showBackButton": true,
        "textResourceBindings": {
          "next": "Neste",
          "back": "Forrige"
        }
      }
    ]
  }
}