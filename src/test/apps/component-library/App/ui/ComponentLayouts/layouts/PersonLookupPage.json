{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": false,
    "layout": [
      {
        "id": "PersonLookupPage-NavigationBar",
        "type": "NavigationBar"
      },
      {
        "id": "personLookup",
        "type": "PersonLookup",
        "dataModelBindings": {
          "person_lookup_ssn": "GridExample.ExampleInputTwo",
          "person_lookup_name": "street"
        },
        "textResourceBindings": {
          "title": "Legg til person",
          "description": "Her legger du inn etternavn og fødselsnummer",
          "help": "For å kunne søke i registeret må du ha fødselsnummeret til personen"
        },
        "required": true
      },
      {
        "id": "PersonLookup-Header-Summary2",
        "type": "Header",
        "textResourceBindings": {
          "title": "Header.Summary2"
        },
        "size": "h3"
      },
      {
        "id": "person-lookup-summary-on-person-lookup-page",
        "type": "Summary2",
        "target": {
          "type": "component",
          "id": "personLookup"
        }
      },
      {
        "id": "PersonLookupPage-NavigationButtons",
        "type": "NavigationButtons",
        "showBackButton": true,
        "textResourceBindings": {
          "next": "Neste",
          "back": "Forrige"
        }
      }
    ]
  }
}