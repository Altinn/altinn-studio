<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xsd:element name="Message" type="MessageV1" />
  <xsd:complexType name="MessageV1">
    <xsd:sequence>
      <xsd:element name="ProcessTask" nillable="true" type="xsd:string" />
      <xsd:element name="Title" nillable="true" type="xsd:string" />
      <xsd:element name="Body" nillable="true" type="xsd:string" />
      <xsd:element name="Reference" nillable="true" type="xsd:string" />
      <xsd:element name="Sender" nillable="true" type="xsd:string" />
      <xsd:element name="IsSomeValue" nillable="true" type="xsd:boolean" />
      <xsd:element name="SomeNumberValue" nillable="true" type="xsd:int" />
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>