{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": [
      "not",
      [
        "commaContains",
        ["dataModel", "ChooseExtraPages"],
        "label-data-bindings"
      ]
    ],
    "layout": [
      {
        "id": "labelDataBindingsNavBar",
        "type": "NavigationBar"
      },
      {
        "id": "colorsCheckboxes",
        "type": "Checkboxes",
        "textResourceBindings": {
          "title": "ChooseColors"
        },
        "dataModelBindings": {
          "simpleBinding": "Colors",
          "label": "ColorsLabels"
        },
        "sortOrder": "asc",
        "optionsId": "colors",
        "required": true,
        "readOnly": false
      },
      {
        "id": "ColorsLabelsVerify",
        "type": "Input",
        "textResourceBindings": {
          "title": "Du valgte:"
        },
        "dataModelBindings": {
          "simpleBinding": "ColorsLabelsVerify"
        },
        "required": false,
        "readOnly": true,
        "grid": {
          "md": 6
        }
      },
      {
        "id": "choose-food",
        "type": "RadioButtons",
        "textResourceBindings": {
          "title": "ChooseFood"
        },
        "dataModelBindings": {
          "simpleBinding": "Food",
          "label": "FoodLabel"
        },
        "sortOrder": "asc",
        "optionsId": "foods",
        "required": true,
        "readOnly": false
      },
      {
        "id": "colorLabel",
        "type": "Input",
        "textResourceBindings": {
          "title": "Du valgte:"
        },
        "dataModelBindings": {
          "simpleBinding": "FoodLabel"
        },
        "required": false,
        "readOnly": true,
        "grid": {
          "md": 6
        }
      }
    ]
  }
}

