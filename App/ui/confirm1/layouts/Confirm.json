{
  "$schema": "https://altinncdn.no/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "layout": [
      {
        "id": "Panel-confirm1",
        "type": "Panel",
        "textResourceBindings": {
          "title": "Form.Panel-confirm1.title",
          "body": "Form.Panel-confirm1.body"
        },
        "variant": "warning",
        "dataModelBindings": {}
      },
      {
        "id": "Input-name",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-name.title",
          "description": "Form.Input-name.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.name"
        },
        "readOnly": true,
        "autocomplete": "organization",
        "grid": {
          "xs": 12,
          "sm": 6
        }
      },
      {
        "id": "Input-orgNumber",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-orgNumber.title",
          "description": "Form.Input-orgNumber.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.orgNumber"
        },
        "readOnly": true,
        "autocomplete": "off",
        "grid": {
          "xs": 12,
          "sm": 6
        }
      },
      {
        "id": "Input-year",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-year.title",
          "description": "Form.Input-year.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.year"
        },
        "readOnly": true,
        "autocomplete": "off",
        "formatting": {
          "align": "right",
          "number": {
            "format": "####",
            "mask": "_",
            "allowEmptyFormatting": true
          }
        },
        "grid": {
          "xs": 12,
          "sm": 6
        }
      },
      {
        "id": "Input-income",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-income.title",
          "description": "Form.Input-income.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.income"
        },
        "readOnly": true,
        "autocomplete": "off",
        "formatting": {
          "align": "right",
          "number": {
            "allowNegative": false,
            "decimalScale": 0,
            "suffix": " 000 NOK",
            "thousandSeparator": " "
          }
        },
        "grid": {
          "xs": 12,
          "sm": 6
        }
      },
      {
        "id": "Paragraph-confirm",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "Confirm.Paragraph-confirm.title"
        },
        "dataModelBindings": {}
      },
      {
        "id": "Button-confirm",
        "type": "Button",
        "textResourceBindings": {
          "title": "Form.Button-confirm.title"
        },
        "dataModelBindings": {}
      }
    ],
    "hidden": false
  }
}
