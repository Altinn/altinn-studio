{
  "$schema": "https://altinncdn.no/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "layout": [
      {
        "id": "altinnLogo",
        "type": "Image",
        "textResourceBindings": {
          "altTextImg": "Altinn logo",
          "help": "Altinn logo"
        },
        "image": {
          "src": {
            "nb": "/ttd/frontend-test/images/altinn-logo.svg"
          },
          "width": "100%",
          "align": "center"
        },
        "grid": {
          "xs": 12
        }
      },
      {
        "id": "messageTitle",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "messageTitle",
          "help": "messageTitleHelp"
        }
      },
      {
        "id": "messageBody",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "messageBody"
        }
      },
      {
        "id": "messageAttachments",
        "type": "AttachmentList",
        "textResourceBindings": {
          "title": "messageAttachments"
        },
        "dataTypeIds": ["fileUpload-message"]
      },
      {
        "id": "startAgain",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "startAgain"
        }
      },
      {
        "id": "input1",
        "type": "Input",
        "textResourceBindings": {
          "title": "Test input 1"
        },
        "dataModelBindings": {
          "simpleBinding": "Reference"
        }
      },
      {
        "id": "input2",
        "type": "Input",
        "textResourceBindings": {
          "title": "Test input 2"
        },
        "dataModelBindings": {
          "simpleBinding": "Body"
        }
      },
      {
        "id": "sendInButton",
        "type": "Button",
        "textResourceBindings": {
          "title": "Send inn"
        }
      }
    ]
  }
}
