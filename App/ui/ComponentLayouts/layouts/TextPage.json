{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": false,
    "layout": [
      {
        "id": "TextPage-NavigationBar",
        "type": "NavigationBar"
      },
      {
        "id": "TextPage-Header",
        "type": "Header",
        "size": "h2",
        "textResourceBindings": {
          "title": "Text"
        }
      },
      {
        "id": "textWithoutLabel",
        "type": "Text",
        "value": "Dette er helt vanlig tekst uten noe merkelapp eller datamodell."
      },
      {
        "id": "firstName",
        "type": "Text",
        "textResourceBindings": {
          "title": "Fornavn"
        },
        "value": "Espen"
      },
      {
        "id": "lastName",
        "type": "Text",
        "textResourceBindings": {
          "title": "Etternavn"
        },
        "value": "Askeladd"
      },
      {
        "id": "street",
        "type": "Text",
        "textResourceBindings": {
          "title": "Gatenavn"
        },
        "value": "Testveien 23",
        "direction": "vertical"
      },
      {
        "id": "postPlace",
        "type": "Text",
        "textResourceBindings": {
          "title": "Postnummer"
        },
        "value": "0123",
        "direction": "vertical",
        "grid": {
          "xs": 6
        }
      },
      {
        "id": "place",
        "type": "Text",
        "textResourceBindings": {
          "title": "Sted"
        },
        "value": "Oslo",
        "direction": "vertical",
        "grid": {
          "xs": 6
        }
      },
      {
        "id": "language",
        "type": "Text",
        "textResourceBindings": {
          "title": "Spr√•k"
        },
        "value": ["language"]
      },
      {
        "type": "Cards",
        "id": "person-Card",
        "color": "subtle",
        "cards": [
          {
            "title": "Person",
            "description": "",
            "children": ["firstName", "lastName", "language"]
          }
        ],
        "grid": {
          "xs": 12,
          "innerGrid": {
            "xs": 6
          }
        }
      },
      {
        "type": "Cards",
        "id": "address-Card",
        "color": "subtle",
        "cards": [
          {
            "title": "Adresse",
            "description": "",
            "children": ["street", "postPlace", "place"]
          }
        ],
        "grid": {
          "xs": 12,
          "innerGrid": {
            "xs": 6
          }
        }
      },
      {
        "id": "Text-NavigationButtons",
        "type": "NavigationButtons",
        "showBackButton": true,
        "textResourceBindings": {}
      }
    ]
  }
}
