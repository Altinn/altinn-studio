{
    "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
    "data": {
        "layout": [
            {
                "id": "NavigationBar",
                "type": "NavigationBar"
            },
            {
                "size": "h2",
                "id": "GroupPage-Header",
                "type": "Header",
                "textResourceBindings": {
                    "title": "GroupPage"
                }
            },
            {
                "size": "h3",
                "id": "GroupPage-Header-Component",
                "type": "Header",
                "textResourceBindings": {
                    "title": "Header.Component"
                }
            },
            {
                "id": "GroupPage-Group",
                "type": "Group",
                "groupingIndicator": "indented",
                "hidden": false,
                "textResourceBindings": {
                    "title": "Group.Title",
                    "description": "Group.Description"
                },
                "children": ["GroupPage-Group-Header", "GroupPage-Input", "GroupPage-Group-Nested"]
            },
            {
                "id": "GroupPage-PanelGroup",
                "type": "Group",
                "textResourceBindings": {
                    "description": "Panel group description"
                },
                "groupingIndicator": "panel",
                "children": ["GroupPage-PanelGroup-Input1", "GroupPage-PanelGroup-Input2", "GroupPage-PanelGroup-Input3"]
            },
            {
                "id": "GroupPage-PanelGroup-Input1",
                "type": "Input",
                "textResourceBindings": {
                    "title": "Input 1"
                },
                "dataModelBindings": {
                    "simpleBinding": "shortAnswerInput"
                },
                "grid": {
                    "xs": 12,
                    "sm": 4
                }
            },
            {
                "id": "GroupPage-PanelGroup-Input2",
                "type": "Input",
                "textResourceBindings": {
                    "title": "Input 2"
                },
                "dataModelBindings": {
                    "simpleBinding": "shortAnswerInput"
                },
                "grid": {
                    "xs": 12,
                    "sm": 4
                }
            },
            {
                "id": "GroupPage-PanelGroup-Input3",
                "type": "Input",
                "textResourceBindings": {
                    "title": "Input 3"
                },
                "dataModelBindings": {
                    "simpleBinding": "shortAnswerInput"
                },
                "grid": {
                    "xs": 12,
                    "sm": 4
                }
            },
            {
                "size": "h3",
                "id": "GroupPage-Header-Summary",
                "type": "Header",
                "textResourceBindings": {
                    "title": "Header.Summary"
                }
            },
            {
                "componentRef": "GroupPage-Group",
                "id": "GroupPage-Summary",
                "type": "Summary"
            },
            {
                "componentRef": "GroupPage-PanelGroup",
                "id": "GroupPage-PanelGroup-Summary",
                "type": "Summary"
            },
            {
                "size": "h3",
                "id": "GroupPage-Header-Summary2",
                "type": "Header",
                "textResourceBindings": {
                    "title": "Header.Summary2"
                }
            },
            {
                "id": "GroupPage-Summary2",
                "type": "Summary2",
                "hidden": false,
                "target": {
                    "type": "component",
                    "id": "GroupPage-Group"
                }
            },
            {
                "id": "GroupPage-PanelGroup-Summary2",
                "type": "Summary2",
                "hidden": false,
                "target": {
                    "type": "component",
                    "id": "GroupPage-PanelGroup"
                }
            },
            {
                "id": "NavigationButtons",
                "showBackButton": true,
                "textResourceBindings": {},
                "type": "NavigationButtons"
            },
            {
                "size": "L",
                "id": "GroupPage-Group-Header",
                "type": "Header",
                "textResourceBindings": {
                    "title": "GroupPage-Group-Header.title"
                }
            },
            {
                "id": "GroupPage-Input",
                "type": "Input",
                "textResourceBindings": {
                    "title": "Required input field inside group"
                },
                "dataModelBindings": {
                    "simpleBinding": "shortAnswerInput"
                },
                "required": false,
                "showValidations": ["All"]
            },
            {
                "id": "GroupPage-Group-Nested",
                "type": "Group",
                "groupingIndicator": "indented",
                "hidden": false,
                "textResourceBindings": {
                    "description": "Nested group description"
                },
                "children": [
                    "GroupPage-Nested-Header",
                    "GroupPage-Nested-Input",
                    "GroupPage-Nested-Input2",
                    "GroupPage-Nested-Textarea",
                    "GroupPage-Nested-Checkboxes",
                    "GroupPage-Group-Nested-Nested"
                ]
            },
            {
                "size": "L",
                "id": "GroupPage-Nested-Header",
                "type": "Header",
                "textResourceBindings": {
                    "title": "I am a nested header"
                }
            },
            {
                "dataModelBindings": {
                    "simpleBinding": "nestedInput"
                },
                "id": "GroupPage-Nested-Input",
                "type": "Input",
                "textResourceBindings": {
                    "title": "I am input inside nested group"
                }
            },
            {
                "dataModelBindings": {
                    "simpleBinding": "nestedInput2"
                },
                "id": "GroupPage-Nested-Input2",
                "type": "Input",
                "textResourceBindings": {
                    "title": "I am nested input 2"
                }
            },
            {
                "dataModelBindings": {
                    "simpleBinding": "longAnswerInput"
                },
                "id": "GroupPage-Nested-Textarea",
                "type": "TextArea",
                "textResourceBindings": {
                    "title": "TextareaPage.Textarea.title"
                }
            },
            {
                "dataModelBindings": {
                    "simpleBinding": "checkboxesInput"
                },
                "id": "GroupPage-Nested-Checkboxes",
                "type": "Checkboxes",
                "textResourceBindings": {
                    "title": "CheckboxesPage.Checkboxes.title"
                },
                "options": [
                    {
                        "label": "Kjøre til hytta på fjellet",
                        "value": "kjoreTilHyttaPaaFjellet"
                    },
                    {
                        "label": "Kjøring i skogen",
                        "value": "kjoringISkogen"
                    },
                    {
                        "label": "Korte strekninger med bykjøring, eller annen moro",
                        "value": "korteStrekningerMedBykjoring"
                    },
                    {
                        "label": "Lange strekninger på større veier i Norge",
                        "value": "langeStrekningerPaStorreVeierINorge"
                    }
                ],
                "required": false
            },
            {
                "id": "GroupPage-Group-Nested-Nested",
                "type": "Group",
                "groupingIndicator": "indented",
                "hidden": false,
                "textResourceBindings": {
                    "title": "Nested group title inside nested group",
                    "description": "Nested nested group description inside nested group"
                },
                "children": ["GroupPage-Nested-Nested-Input", "GroupPage-Nested-Nested-RadioButtons"]
            },
            {
                "dataModelBindings": {
                    "simpleBinding": "nestedInput3"
                },
                "id": "GroupPage-Nested-Nested-Input",
                "type": "Input",
                "textResourceBindings": {
                    "title": "I am input inside nested group inside nested group"
                }
            },
            {
                "dataModelBindings": {
                    "simpleBinding": "radioButtonInput"
                },
                "id": "GroupPage-Nested-Nested-RadioButtons",
                "type": "RadioButtons",
                "textResourceBindings": {
                    "title": "RadioButtonsPage.RadioButtons.title"
                },
                "options": [
                    {
                        "label": "Bil",
                        "value": "bil"
                    },
                    {
                        "label": "Moped",
                        "value": "moped"
                    },
                    {
                        "label": "Traktor",
                        "value": "traktor"
                    },
                    {
                        "label": "Båt",
                        "value": "baat"
                    }
                ],
                "required": false
            }
        ]
    }
}
