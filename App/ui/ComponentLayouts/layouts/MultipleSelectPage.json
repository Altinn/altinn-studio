{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": false,
    "layout": [
      {
        "id": "MultipleSelectPage-NavigationBar",
        "type": "NavigationBar"
      },
      {
        "id": "MultipleSelectPage-Header",
        "type": "Header",
        "size": "h2",
        "textResourceBindings": {
          "title": "CheckboxesPage"
        }
      },
      {
        "id": "MultipleSelectPage-Header-Component",
        "type": "Header",
        "size": "h3",
        "textResourceBindings": {
          "title": "Header.Component"
        }
      },
      {
        "id": "MultipleSelectPage-Checkboxes",
        "type": "MultipleSelect",
        "dataModelBindings": {
          "simpleBinding": "multipleSelect"
        },
        "textResourceBindings": {
          "title": "MultipleSelectPage.MultipleSelect.title"
        },
        "alertOnChange": true,
        "options": [
          {
            "label": "Kjøre til hytta på fjellet",
            "value": "kjoreTilHyttaPaaFjellet"
          },
          {
            "label": "Kjøring i skogen",
            "value": "kjoringISkogen"
          },
          {
            "label": "Korte strekninger med bykjøring, eller annen moro",
            "value": "korteStrekningerMedBykjoring"
          },
          {
            "label": "Lange strekninger på større veier i Norge",
            "value": "langeStrekningerPaStorreVeierINorge"
          }
        ],
        "required": false
      },
      {
        "id": "MultipleSelectPage-Header-Summary",
        "type": "Header",
        "size": "h3",
        "textResourceBindings": {
          "title": "Header.Summary"
        }
      },
      {
        "id": "MultipleSelectPage-Summary",
        "componentRef": "MultipleSelectPage-Checkboxes",
        "type": "Summary",
        "largeGroup": false
      },
      {
        "id": "MultipleSelectPage-Header-Summary2",
        "type": "Header",
        "size": "h3",
        "textResourceBindings": {
          "title": "Header.Summary2"
        }
      },
      {
        "id": "MultipleSelectPage-Header-Summary2-Display-Auto",
        "type": "Header",
        "size": "h4",
        "textResourceBindings": {
          "title": "displayType: NULL (Auto)"
        }
      },
      {
        "id": "MultipleSelectPage-Summary2-Display-Auto",
        "type": "Summary2",
        "hidden": false,
        "target": {
          "type": "component",
          "id": "MultipleSelectPage-Checkboxes"
        }
      },
      {
        "id": "MultipleSelectPage-Header-Summary2-Display-String",
        "type": "Header",
        "size": "h4",
        "textResourceBindings": {
          "title": "displayType: String"
        }
      },
      {
        "id": "MultipleSelectPage-Summary2-Display-String",
        "type": "Summary2",
        "hidden": false,
        "target": {
          "type": "component",
          "id": "MultipleSelectPage-Checkboxes"
        },
        "overrides": [
          {
            "componentId": "MultipleSelectPage-Checkboxes",
            "displayType": "string"
          }
        ]
      },
      {
        "id": "MultipleSelectPage-Header-Summary2-Display-List",
        "type": "Header",
        "size": "h4",
        "textResourceBindings": {
          "title": "displayType: List"
        }
      },
      {
        "id": "MultipleSelectPage-Summary2-Display-List",
        "type": "Summary2",
        "hidden": false,
        "target": {
          "type": "component",
          "id": "MultipleSelectPage-Checkboxes"
        },
        "overrides": [
          {
            "componentId": "MultipleSelectPage-Checkboxes",
            "displayType": "list"
          }
        ]
      },
      {
        "id": "MultipleSelectPage-Header2",
        "type": "Header",
        "size": "h2",
        "textResourceBindings": {
          "title": "Avkryssningsbokser mot datamodell"
        }
      },
      {
        "dataModelBindings": {
          "group": "MultiselectGroupExample",
          "simpleBinding": "MultiselectGroupExample.Name.firstname",
          "checked": "MultiselectGroupExample.Internal.isChecked"
        },
        "deletionStrategy":"soft",
        "id": "MultipleSelectPage-Checkboxes2",
        "type": "MultipleSelect",
        "textResourceBindings": {
          "title": "Velg personer"
        },
        "optionsId": "persons",
        "required": true
      },
      {
        "id": "MultipleSelectPage-RepeatingGroup",
        "type": "RepeatingGroup",
        "textResourceBindings": {
          "title": "Repeterende Gruppe"
        },
        "dataModelBindings": {
          "group": "MultiselectGroupExample"
        },
        "hidden": false,
        "hiddenRow": ["equals",["dataModel", "MultiselectGroupExample.Internal.isChecked"], false],
        "children": [
          "MultipleSelectPage-RepeatingGroupName",
          "MultipleSelectPage-RepeatingGroupAge"
        ]
      },
      {
        "id": "MultipleSelectPage-RepeatingGroupName",
        "type": "Text",
        "textResourceBindings": {
          "title":"Name"
        },
        "value": ["dataModel","MultiselectGroupExample.Name.firstname"]
      },
      {
        "id": "MultipleSelectPage-RepeatingGroupAge",
        "type": "Input",
        "textResourceBindings": {
          "title":"Age"
        },
        "dataModelBindings": {
          "simpleBinding":"MultiselectGroupExample.age"
        }
      },
      {
        "id": "MultipleSelectPage-Header-Summary-Component2",
        "type": "Header",
        "size": "h3",
        "textResourceBindings": {
          "title": "Header.Summary"
        }
      },
      {
        "id": "MultipleSelectPage-Summary-Component2",
        "type": "Summary",
        "componentRef": "MultipleSelectPage-Checkboxes2"
      },
      {
        "id": "MultipleSelectPage-Header-Summary2-Component2",
        "type": "Header",
        "size": "h3",
        "textResourceBindings": {
          "title": "Header.Summary2"
        }
      },
      {
        "id": "MultipleSelectPage-Summary2-Component2",
        "type": "Summary2",
        "isCompact": true,
        "target": {
          "type": "component",
          "id": "MultipleSelectPage-Checkboxes2"
        }
      },
      {
        "id": "MultipleSelectPage-NavigationButtons",
        "showBackButton": true,
        "textResourceBindings": {},
        "type": "NavigationButtons"
      }
    ]
  }
}
