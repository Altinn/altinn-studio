{
  "$schema": "https://altinncdn.no/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "layout": [
      {
        "id": "Header-form",
        "type": "Header",
        "textResourceBindings": {
          "title": "Form.Header-form.title"
        },
        "dataModelBindings": {},
        "size": "L"
      },
      {
        "id": "Input-name",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-name.title",
          "description": "Form.Input-name.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.name"
        },
        "required": true,
        "readOnly": true,
        "autocomplete": "off",
        "grid": {
          "xs": 6
        }
      },
      {
        "id": "Input-orgNumber",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-orgNumber.title",
          "description": "Form.Input-orgNumber.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.orgNumber"
        },
        "required": true,
        "readOnly": true,
        "autocomplete": "off",
        "grid": {
          "xs": 6
        }
      },
      {
        "id": "Input-year",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-year.title",
          "description": "Form.Input-year.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.year"
        },
        "required": true,
        "readOnly": false,
        "autocomplete": "off",
        "formatting": {
          "align": "right",
          "number": {
            "allowNegative": false,
            "decimalScale": 0
          }
        },
        "grid": {
          "xs": 6
        }
      },
      {
        "id": "Input-income",
        "type": "Input",
        "textResourceBindings": {
          "title": "Form.Input-income.title",
          "description": "Form.Input-income.description"
        },
        "dataModelBindings": {
          "simpleBinding": "form.income"
        },
        "required": true,
        "readOnly": false,
        "autocomplete": "off",
        "formatting": {
          "align": "right",
          "number": {
            "allowNegative": false,
            "decimalScale": 0,
            "suffix": " 000 NOK",
            "thousandSeparator": " "
          }
        },
        "grid": {
          "xs": 6
        }
      },
      {
        "id": "Button-submit",
        "type": "Button",
        "textResourceBindings": {
          "title": "Form.Button-submit.title"
        },
        "dataModelBindings": {}
      }
    ],
    "hidden": false
  }
}