{
  "$schema": "https://altinncdn.no/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "layout": [
      {
        "id": "listComponent",
        "type": "List",
        "textResourceBindings": {
          "title": "Hvem gjelder saken?"
        },
        "dataModelBindings": {
          "name": "SelectedItem"
        },
        "dataListId": "cases",
        "fieldToStoreInDataModel": "idapplication",
        "tableHeaders": [ "Navn", "Alder", "Yrke" ],
