{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": [
      "not",
      ["commaContains", ["dataModel", "ChooseExtraPages"], "numeric-fields"]
    ],
    "layout": [
      {
        "id": "numericFieldNavBar",
        "type": "NavigationBar"
      },
      {
        "type": "Input",
        "id": "decimalAsNumber",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Decimal"
        },
        "textResourceBindings": {
          "title": "Decimal (store tall, desimaltall)",
          "description": "Hvor mange flis har du i fingeren?"
        },
        "formatting": {
          "number": {
            "suffix": " flis",
            "thousandSeparator": " ",
            "decimalSeparator": ","
          },
          "align": "right"
        },
        "showValidations": ["AllExceptRequired"],
        "grid": {
          "xs": 6
        }
      },
      {
        "type": "Input",
        "id": "decimalAsString",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Decimal"
        },
        "textResourceBindings": {
          "title": "String",
          "description": "Samme tall, som en streng"
        },
        "grid": {
          "xs": 6
        },
        "readOnly": true
      },
      {
        "type": "Input",
        "id": "int32AsNumber",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Int32"
        },
        "textResourceBindings": {
          "title": "Int32 (store tall, ikke desimaltall)",
          "description": "Hvor mange ganger sparka du lillet√•a i bordbeinet?"
        },
        "formatting": {
          "number": {
            "suffix": " ganger",
            "thousandSeparator": " ",
            "decimalSeparator": ","
          },
          "align": "right"
        },
        "showValidations": ["AllExceptRequired"],
        "grid": {
          "xs": 6
        }
      },
      {
        "type": "Input",
        "id": "int32AsString",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Int32"
        },
        "textResourceBindings": {
          "title": "String",
          "description": "Samme tall, som en streng"
        },
        "grid": {
          "xs": 6
        },
        "readOnly": true
      },
      {
        "type": "Input",
        "id": "int64AsNumber",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Int64"
        },
        "textResourceBindings": {
          "title": "Int64 (store tall, ikke desimaltall)",
          "description": "Hvor mange ganger har du hatt hodepine denne uken?"
        },
        "formatting": {
          "number": {
            "suffix": " ganger",
            "thousandSeparator": " ",
            "decimalSeparator": ","
          },
          "align": "right"
        },
        "showValidations": ["AllExceptRequired"],
        "grid": {
          "xs": 6
        }
      },
      {
        "type": "Input",
        "id": "int64AsString",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Int64"
        },
        "textResourceBindings": {
          "title": "String",
          "description": "Samme tall, som en streng"
        },
        "grid": {
          "xs": 6
        },
        "readOnly": true
      },
      {
        "type": "Input",
        "id": "int16AsNumber",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Int16"
        },
        "textResourceBindings": {
          "title": "Int16 (mindre tall, ikke desimaltall)",
          "description": "Hvor mange myggstikk fikk du?"
        },
        "formatting": {
          "number": {
            "suffix": " stikk",
            "thousandSeparator": " ",
            "decimalSeparator": ","
          },
          "align": "right"
        },
        "showValidations": ["AllExceptRequired"],
        "grid": {
          "xs": 6
        }
      },
      {
        "type": "Input",
        "id": "int16AsString",
        "dataModelBindings": {
          "simpleBinding": "Numeric.Int16"
        },
        "textResourceBindings": {
          "title": "String",
          "description": "Samme tall, som en streng"
        },
        "grid": {
          "xs": 6
        },
        "readOnly": true
      },
      {
        "id": "numericFieldNavigate",
        "type": "NavigationButtons",
        "textResourceBindings": {
          "back": "Tilbake",
          "next": "Neste"
        },
        "showBackButton": true
      }
    ]
  }
}

