{
  "$schema": "http://localhost:8080/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": [
      "not",
      ["commaContains", ["dataModel", "ChooseExtraPages"], "map"]
    ],
    "layout": [
      {
        "id": "mapNavBar",
        "type": "NavigationBar"
      },
      {
        "id": "map",
        "type": "Map",
        "textResourceBindings": {
          "title": "Velg lokasjon",
          "description": "Lokasjon for hvor du skal endre navn",
          "help": "Trykk p√• kartet"
        },
        "centerLocation": {
          "latitude": 59.93080398894555,
          "longitude": 10.8012469132984
        },
        "zoom": 6,
        "dataModelBindings": {
          "simpleBinding": "MapData.Location",
          "geometries": "MapData.Geometries"
        },
        "geometryType": "WKT",
        "required": true
      },
      {
        "id": "mapSummaryHeader",
        "type": "Header",
        "textResourceBindings": {
          "title": "Oppsummering"
        },
        "size": "h2"
      },
      {
        "id": "mapSummary",
        "type": "Summary",
        "componentRef": "map"
      },
      {
        "id": "mapValue",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": [
            "concat",
            "<b>Din valgte lokasjon</b>:\n\n",
            ["displayValue", "map"]
          ]
        }
      },
      {
        "id": "mapNavigate",
        "type": "NavigationButtons",
        "textResourceBindings": {
          "back": "Tilbake",
          "next": "Neste"
        },
        "showBackButton": true
      }
    ]
  }
}
