        "id": "summaries-header",
        "type": "Header",
        "textResourceBindings": {
          "title": "Oppsummeringer av utvalgte felt"
        },
        "size": "M"
      },
      {
        "id": "single-number-summary",
        "type": "Summary2",
        "target": { "id": "number4" }
      },
      {
        "id": "single-date-summary",
        "type": "Summary2",
        "target": { "id": "date4" }
      },
      {
        "id": "single-text-summary",
        "type": "Summary2",
        "target": { "id": "text2" }
      },
      {
        "id": "single-option-summary",
