{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": [
      "not",
      ["commaContains", ["dataModel", "ChooseExtraPages"], "cards"]
    ],
    "layout": [
      {
        "id": "cardsNavBar",
        "type": "NavigationBar"
      },
      {
        "type": "Cards",
        "id": "patent-cards",
        "color": "subtle",
        "cards": [
          {
            "title": "Varemerke",
            "description": "Dette er et eksempel på hvordan et varemerke kan se ut. Dette kan være en logo, et navn, en farge eller en kombinasjon av disse.",
            "media": "patentstyret-varemerke"
          },
          {
            "title": "Lydmerke",
            "description": "Et lydmerke kan være en jingle, en melodi eller en annen lyd. Et opptak av at noen brøler firmanavnet ditt godtar vi ikke.",
            "media": "patentstyret-lydmerke"
          },
          {
            "title": "Videomerke",
            "description": "Videomerker er en kombinasjon bilde og av og til lyd. Dette kan være en reklamefilm, en animasjon eller en annen type video.",
            "media": "patentstyret-videomerke"
          },
          {
            "title": "Andre",
            "description": "Vi kan registrere alt mulig rart som et kjennemerke. Hvis du vil registere gode kaker, kryss av her så tar vi kontakt for prøvesmaking.",
            "children": ["cardsCheckboxes"]
          }
        ]
      },
      {
        "id": "patentstyret-varemerke",
        "type": "Image",
        "textResourceBindings": {
          "altTextImg": "Varemerke-alt-text"
        },
        "image": {
          "align": "center",
          "width": "100%",
          "src": {
            "nb": "assets/patentstyret-varemerke.jpg"
          }
        }
      },
      {
        "id": "patentstyret-lydmerke",
        "type": "Audio",
        "textResourceBindings": {
          "altText": "Lydmerke-alt-text"
        },
        "audio": {
          "src": {
            "nb": "assets/patentstyret-lydmerke.mp3"
          }
        }
      },
      {
        "id": "patentstyret-videomerke",
        "type": "Video",
        "textResourceBindings": {
          "altText": "Videomerke-alt-text"
        },
        "video": {
          "src": {
            "nb": "assets/patentstyret-videomerke.mp4"
          }
        }
      },
      {
        "id": "cardsCheckboxes",
        "type": "Checkboxes",
        "dataModelBindings": {
          "simpleBinding": "Innledning-grp-9309.Signerer-grp-9320.SignererEkstraAksept-datadef-34754.value"
        },
        "options": [
          {
            "label": "Jeg inviterer Patentstyret til å smake på kaker",
            "value": "J"
          }
        ]
      },
      {
        "id": "number1",
        "type": "Number",
        "textResourceBindings": {
          "title": "Total gjeld"
        },
        "value": ["dataModel","GridData.TotalGjeld"],
        "direction": "horizontal",
        "formatting": {
          "currency": "NOK",
          "position": "suffix",
          "number": {
            "thousandSeparator": " "
          }
        },
        "grid": {
          "xs": 12
        }
      },
      {
        "id": "number2",
        "type": "Number",
        "textResourceBindings": {
          "title": "Statisk verdi som tall"
        },
        "value": 2000,
        "direction": "horizontal",
        "formatting": {
          "currency": "NOK",
          "position": "suffix",
          "number": {
            "thousandSeparator": " "
          }
        },
        "grid": {
          "xs": 12
        }
      },
      {
        "id": "number3",
        "type": "Number",
        "textResourceBindings": {
          "title": "Kredittkort prosent",
          "description": "",
          "helpText": ""
