        "value": ["dataModel","GridData.Kredittkort.Prosent"],
        "direction": "horizontal",
        "formatting": {
          "unit": "percent",
          "position": "suffix",
          "number": {
            "thousandSeparator": " "
          }
        },
        "grid": {
          "xs": 12
        }
      },
      {
        "id": "number4",
        "type": "Number",
        "textResourceBindings": {
          "title": "Statisk verdi med desimal",
          "description": "Dette er en beskrivelse",
          "help":"Dette er en helptext"
        },
        "value": 20000.20,
        "direction": "horizontal",
        "formatting": {
          "currency": "NOK",
          "position": "suffix",
          "number": {
            "thousandSeparator": " "
          }
        },
        "grid": {
          "xs": 12
        }
