{
    "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
    "data": {
      "hidden": false,
      "layout": [
        {
          "id": "ListPage-NavigationBar",
          "type": "NavigationBar"
        },
        {
          "id": "ListPage-Header",
          "type": "Header",
          "size": "h2",
          "textResourceBindings": {
            "title": "ListPage"
          }
        },
        {
          "id": "ListPage-Header-Component",
          "type": "Header",
          "size": "h3",
          "textResourceBindings": {
            "title": "Header.Component"
          }
        },
        {
          "id": "ListPage-ListComponent",
          "type": "List",
          "dataModelBindings": {
            "name": "list"
          },
          "textResourceBindings": {
            "title": "ListPage.List.title"
          },
          "bindingToShowInSummary": "list",
          "dataListId": "people",
          "tableHeaders": {
            "name": "Navn",
            "age": "Alder",
            "profession": "Yrke"
          },
          "sortableColumns": ["age"],
          "pagination": {
            "alternatives": [5, 10],
            "default": 5
          },
          "mapping": {
            "Search": "search"
          }
        },
        {
          "id": "ListPage-Header-Summary",
          "type": "Header",
          "size": "h3",
          "textResourceBindings": {
            "title": "Header.Summary"
          }
        },
        {
          "id": "ListPage-Summary",
          "type": "Summary",
          "componentRef": "ListPage-ListComponent"
        },
        {
          "id": "ListPage-Header-Summary2",
          "type": "Header",
          "size": "h3",
          "textResourceBindings": {
            "title": "Header.Summary2"
          }
        },
        {
          "id": "ListPage-Summary2",
          "type": "Summary2",
          "target": {
            "type": "component",
            "id": "ListPage-ListComponent"
          }
        },
        {
          "id": "ListPage-NavigationButtons",
          "showBackButton": true,
          "textResourceBindings": {},
          "type": "NavigationButtons"
        }
      ]
    }
  }