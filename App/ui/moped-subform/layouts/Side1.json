{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/layout/layout.schema.v1.json",
  "data": {
    "hidden": false,
    "layout": [
      {
        "size": "L",
        "id": "moped-header",
        "type": "Header",
        "textResourceBindings": {
          "title": "moped-header.title"
        }
      },
      {
        "id": "moped-blurb",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "moped-blurb.title"
        }
      },
      {
        "dataModelBindings": {
          "simpleBinding": "RegNo"
        },
        "id": "moped-regno",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-regno.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Merke"
        },
        "id": "moped-merke",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-merke.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Modell"
        },
        "id": "moped-modell",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-modell.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Produksjonsaar"
        },
        "id": "moped-produksjonsaar",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-produksjonsaar.title"
        },
        "formatting": {
          "number": {
            "thousandSeparator": ""
          },
          "align": "right"
        },
        "required": true
      },
      {
        "id": "backbutton",
        "type": "CustomButton",
        "textResourceBindings": {
          "title": "general.next"
        },
        "actions": [
          {
            "type": "ClientAction",
            "id": "nextPage"
          }
        ],
        "buttonStyle": "primary",
        "buttonColor": "first"
      }
    ]
  }
}