{
  "$schema": "http://localhost:8080/schemas/json/layout/layout.schema.v1.json?dummy=2",
  "data": {
    "hidden": false,
    "layout": [
      {
        "size": "L",
        "id": "moped-header",
        "type": "Header",
        "textResourceBindings": {
          "title": "moped-header.title"
        }
      },
      {
        "id": "moped-blurb",
        "type": "Paragraph",
        "textResourceBindings": {
          "title": "moped-blurb.title"
        }
      },
      {
        "dataModelBindings": {
          "simpleBinding": "RegNo"
        },
        "id": "moped-regno",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-regno.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Merke"
        },
        "id": "moped-merke",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-merke.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Modell"
        },
        "id": "moped-modell",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-modell.title"
        },
        "required": true
      },
      {
        "dataModelBindings": {
          "simpleBinding": "Produksjonsaar"
        },
        "id": "moped-produksjonsaar",
        "type": "Input",
        "textResourceBindings": {
          "title": "moped-produksjonsaar.title"
        },
        "formatting": {
          "number": {
            "format": "####"
          }
        },
        "required": true
      },
      {
        "id": "button-group2",
        "type": "ButtonGroup",
        "children": ["subform-cancelButton", "subform-nextButton"]
      },
      {
        "id": "subform-cancelButton",
        "type": "CustomButton",
        "textResourceBindings": {
          "title": "general.cancel"
        },
        "actions": [
          {
            "type": "ClientAction",
            "id": "closeSubForm"
          }
        ],
        "buttonSize": "sm",
        "buttonStyle": "secondary",
        "buttonColor": "second"
      },
      {
        "id": "subform-nextButton",
        "type": "CustomButton",
        "textResourceBindings": {
          "title": "general.next"
        },
        "actions": [
          {
            "type": "ClientAction",
            "id": "nextPage",
            "validation": {
              "page": "all",
              "show": ["All"]
            }
          }
        ],
        "buttonStyle": "primary",
        "buttonColor": "first",
        "buttonSize": "sm"
      }
    ]
  }
}