{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://altinn-repositories:3000/bjosttveit/v4-data/App/models/model.schema.json",
  "info": {
    "rootNode": ""
  },
  "@xsdNamespaces": {
    "xsd": "http://www.w3.org/2001/XMLSchema",
    "xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "seres": "http://seres.no/xsd/forvaltningsdata"
  },
  "@xsdSchemaAttributes": {
    "AttributeFormDefault": "Unqualified",
    "ElementFormDefault": "Qualified",
    "BlockDefault": "None",
    "FinalDefault": "None"
  },
  "@xsdRootElement": "model",
  "type": "object",
  "required": [
    "streetnr",
    "postnr"
  ],
  "$defs": {
    "GridExample": {
      "type": "object",
      "properties": {
        "ExampleDate": {
          "type": "string"
        },
        "ExampleDropdown": {
          "type": "string"
        },
        "ExampleMultiSelect": {
          "type": "string"
        },
        "ExampleTextArea": {
          "type": "string"
        },
        "ExampleInput": {
          "type": "string",
          "maxLength": 7
        },
        "ExampleRadio": {
          "type": "string"
        },
        "ExampleCheckbox": {
          "type": "string"
        },
        "ExampleInputTwo": {
          "type": "string"
        },
        "ExampleInputThree": {
          "type": "string"
        },
        "ExampleRadioTwo": {
          "type": "string"
        },
        "ExampleRadioThree": {
          "type": "string"
        },
        "ExampleCheckboxTwo": {
          "type": "string"
        },
        "ExampleCheckboxThree": {
          "type": "string"
        }
      }
    },
    "RepeatingGroupExample": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "points": {
          "type": "integer",
          "maximum": 100
        },
        "date": {
          "type": "string",
          "format": "date"
        }
      }
    },
    "RepeatingGroupWithNestedGroup": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "points": {
          "type": "integer"
        },
        "date": {
          "type": "string",
          "format": "date"
        },
        "cars": {
          "type": "array",
          "maxItems": 3,
          "items": {
            "$ref": "#/$defs/NestedGroup"
          }
        }
      }
    },
    "NestedGroup": {
      "type": "object",
      "properties": {
        "carBrand": {
          "type": "string"
        },
        "carModel": {
          "type": "string"
        },
        "modelYear": {
          "type": "integer"
        }
      }
    }
  },
  "properties": {
    "inputfield": {
      "type": "string"
    },
    "shortAnswerInput": {
      "type": "string"
    },
    "longAnswerInput": {
      "type": "string"
    },
    "radioButtonInput": {
      "type": "string"
    },
    "checkboxesInput": {
      "type": "string",
      "pattern": "[A-Za-z,æøå]*(kjoringISkogen)[A-Za-z,æøå]*"
    },
    "nestedInput": {
      "type": "string"
    },
    "nestedInput2": {
      "type": "string"
    },
    "nestedInput3": {
      "type": "string"
    },
    "streetnr": {
      "type": "string"
    },
    "postnr": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "co": {
      "type": "string"
    },
    "street": {
      "type": "string"
    },
    "GridExample": {
      "$ref": "#/$defs/GridExample"
    },
    "dropdown": {
      "type": "string"
    },
    "multipleSelect": {
      "type": "string"
    },
    "repeatingGroup": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/RepeatingGroupExample"
      }
    },
    "nestedRepeatingGroup": {
      "type": "array",
      "maxItems": 3,
      "items": {
        "$ref": "#/$defs/RepeatingGroupWithNestedGroup"
      }
    }
  }
}