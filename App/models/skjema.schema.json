          "minItems": 0,
          "items": {
            "type": "object",
            "properties": {
              "arbeidsgiver": {
                "type": "string"
              },
              "fra": {
                "type": "string"
              },
              "til": {
                "type": "string"
              },
              "stilling": {
                "type": "string"
              },
              "beskrivelse": {
                "type": "string"
              },
              "prosjekter": {
                "type": "array",
                "maxItems": 99,
                "minItems": 0,
                "items": {
                  "type": "object",
                  "properties": {
                    "tittel": {
                      "type": "string",
                      "maxLength": 32
                    },
                    "beskrivelse": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "tittel"
                  ]
                }
              }
            },
            "required": [
              "arbeidsgiver",
              "fra",
              "til",
              "stilling"
            ]
          }
        },
        "epost": {
          "type": "string",
          "format": "idn-email"
        },
        "telefonnummer": {
          "type": "integer"
        },
        "bosted": {
          "type": "string"
        },
        "kjønn": {
          "type": "string"
        },
        "SF_skjul-felter": {
          "type": "string"
        },
        "SF_skjul-arbeidsgiver": {
          "type": "string"
        },
        "SF_skjul-prosjekt": {
          "type": "string"
        }
      },
      "required": [
        "fornavn",
        "etternavn",
        "alder",
        "epost",
        "telefonnummer",
        "bosted",
        "kjønn"
      ]
    }
