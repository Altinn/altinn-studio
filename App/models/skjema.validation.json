{
  "$schema": "https://altinncdn.no/toolkits/altinn-app-frontend/4/schemas/json/validation/validation.schema.v1.json",
  "validations": {
    "root.alder": [
      {
        "message": "Kandidater under 18 år må ha foresattes skriftlige samtykke",
        "severity": "warning",
        "condition": [
          "and",
          ["greaterThan", ["stringLength", ["dataModel", ["argv", 0]]], 0],
          ["greaterThanEq", ["dataModel", ["argv", 0]], 15],
          ["lessThan", ["dataModel", ["argv", 0]], 18]
        ]
      }
    ],
    "root.telefonnummer": [
      {
        "message": ["concat", ["text", "test1"], ["text", "test2"]], 
        "condition": [
          "and",
          ["greaterThan", ["stringLength", ["dataModel", ["argv", 0]]], 0],
          ["not", ["startsWith", ["dataModel", ["argv", 0]], "9"]]
        ]
      }
    ],
    "root.epost": [
      {
        "message": "E-post må slutte med '@altinn.no'",
        "condition": [
          "and",
          ["greaterThan", ["stringLength", ["dataModel", ["argv", 0]]], 0],
          ["not", ["endsWith", ["dataModel", ["argv", 0]], "@altinn.no"]]
        ]
      }
    ],
    "root.arbeidserfaring.beskrivelse": [
      {
        "message": "Beskrivelse kan ikke være flink",
        "condition": [
          "and",
          ["greaterThan", ["stringLength", ["dataModel", "root.arbeidserfaring.beskrivelse"]], 0],
          ["equals", ["dataModel", "root.arbeidserfaring.beskrivelse"], "flink"]
        ]
      }
    ],
    "root.arbeidserfaring.prosjekter.beskrivelse": [
      {
        "message": "Beskrivelse kan ikke være kult",
        "condition": [
          "and",
          ["greaterThan", ["stringLength", ["dataModel", ["argv", 0]]], 0],
          ["equals", ["dataModel", ["argv", 0]], "kult"]
        ]
      }
    ]
  },
  "definitions": {}
}
