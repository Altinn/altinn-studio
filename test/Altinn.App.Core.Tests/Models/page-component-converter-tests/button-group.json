{
  "valid": true,
  "layout": {
    "$schema": "https://altinncdn.no/schemas/json/layout/layout.schema.v1.json",
    "data": {
      "layout": [
        {
          "id": "buttongroup",
          "type": "ButtonGroup",
          "children": [
            "nav-buttons",
            "submit",
            "print",
            "instantiate",
            "redirect",
            "action"
          ]
        },
        {
          "id": "redirect",
          "type": "Link",
          "textResourceBindings": {
            "title": "Gå til signering",
            "target": [
              "concat",
              "https://altinn.no/?instance=",
              [
                "instanceContext",
                "instanceId"
              ]
            ]
          },
          "openInNewTab": false,
          "style": "primary"
        },
        {
          "id": "nav-buttons",
          "type": "NavigationButtons",
          "textResourceBindings": {
            "next": "Gå videre",
            "back": "Tilbake"
          },
          "triggers": [
            "validatePage"
          ],
          "dataModelBindings": {},
          "showBackButton": true
        },
        {
          "id": "submit",
          "type": "Button",
          "textResourceBindings": {
            "title": "Send inn"
          }
        },
        {
          "id": "action",
          "type": "ActionButton",
          "textResourceBindings": {
            "title": "Signer"
          },
          "action": "sign",
          "buttonStyle": "primary"
        },
        {
          "id": "print",
          "type": "PrintButton",
          "textResourceBindings": {
            "title": "Print til PDF"
          }
        },
        {
          "id": "instantiate",
          "type": "InstantiationButton",
          "textResourceBindings": {
            "title": "Instansier"
          }
        }
      ]
    }
  }
}
