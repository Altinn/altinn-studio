<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

  <PropertyGroup>
    <CodeAnalysisCompatibleVersion>4.5.0</CodeAnalysisCompatibleVersion>
  </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" >
        <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        <PrivateAssets>all</PrivateAssets>
      </PackageReference>
      <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="$(CodeAnalysisCompatibleVersion)" />
      <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="$(CodeAnalysisCompatibleVersion)" />
      <PackageReference Include="Microsoft.CodeAnalysis.Workspaces.Common" Version="$(CodeAnalysisCompatibleVersion)" />
      <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" Version="$(CodeAnalysisCompatibleVersion)" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Designer\Designer.csproj" />
    </ItemGroup>

</Project>
