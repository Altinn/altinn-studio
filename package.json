    "moment": "2.29.4",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react": "18.2.0",
    "react-content-loader": "6.2.0",
    "react-device-detect": "2.2.2",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-redux": "8.0.5",
    "react-router-dom": "6.8.0",
    "react-select": "5.7.0",
    "redux": "4.2.1",
    "redux-saga": "1.2.2",
    "reselect": "4.1.7",
    "typescript": "4.9.5",
    "uuid": "9.0.0"
  },
  "packageManager": "yarn@3.4.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      ".husky/pre-commit-check-for-skipped-tests",
      "eslint --cache --fix"
    ],
    "*.{json}": "eslint --cache --fix",
    "*.{css,md}": "prettier --write"
  },
  "jest": {
    "transform": {
      "\\.(js|ts|tsx)$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.ts",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\./applicationMetadataMock.json": "<rootDir>/src/__mocks__/applicationMetadataMock.json",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^uuid$": "<rootDir>/node_modules/uuid/dist/index.js"
    },
    "testRegex": "\\.test\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "testEnvironmentOptions": {
      "url": "https://local.altinn.cloud/ttd/test"
    },
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!react-leaflet)/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ]
  }
}
