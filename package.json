    "mini-css-extract-plugin": "2.7.6",
    "msw": "1.3.1",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redux-mock-store": "1.5.4",
    "redux-saga": "1.2.3",
    "redux-saga-test-plan": "4.0.6",
    "source-map-loader": "4.0.1",
    "style-loader": "3.3.3",
    "svg-inline-loader": "0.8.2",
    "swc-loader": "0.2.3",
    "terser-webpack-plugin": "5.3.9",
