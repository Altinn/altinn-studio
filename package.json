    "react-dom": "18.2.0",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.7",
    "redux-mock-store": "1.5.4",
    "redux-saga-test-plan": "4.0.6",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "4.1.2",
    "source-map-loader": "4.0.1",
