  },
  "packageManager": "yarn@4.2.2",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      ".husky/pre-commit-check-for-skipped-tests",
      "eslint --cache --fix"
    ],
    "*.{json}": "eslint --cache --fix",
    "*.{css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ]
  },
  "percy": {
    "version": 2,
    "discovery": {
      "allowed-hostnames": [
        "localhost",
        "localhost:8080",
        "altinncdn.no"
      ],
      "launch-options": {
        "executable": "/usr/bin/google-chrome",
        "timeout": 30000
      }
    }
  }
}
