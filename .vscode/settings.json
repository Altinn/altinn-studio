// Place your settings in this file to overwrite default and user settings.
{
  "[csharp]": {
    "editor.tabSize": 4,
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.rulers": [
    120
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/bin": true,
    "**/obj": true,
    "**/publish/**": true,
    "**/Debug": true
  }
}
